{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { 
      "src": "/(.*)",
      "dest": "/api/index.php"
    },
    { 
      "src": "api/style.css",
      "dest": "/api/style.css"
    },
    { 
      "src": "api/uploads/(.*)png",
      "dest": "/api/uploads/$1"
    },
    { 
      "src": "api/uploads/(.*)jpg",
      "dest": "/api/uploads/$1"
    },
    { 
      "src": "api/index.php",
      "dest": "/api/index.php"
    },
    { 
      "src": "api/portfolio/add.php",
      "dest": "/api/portfolio/add.php"
    },
    { 
      "src": "api/portfolio/delete.php",
      "dest": "/api/portfolio/delete.php"
    },
    { 
      "src": "api/portfolio/edit.php",
      "dest": "/api/portfolio/edit.php"
    },
    { 
      "src": "api/includes/db_connection.php",
      "dest": "/api/includes/db_connection.php"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "http://127.0.0.1:8080" }
  ]
}
