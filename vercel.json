{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
      {
        "src": "/api/public/style.css",
        "dest": "/api/public/style.css"
      },
      {
        "src": "/api/public/uploads/(.*).png",
        "dest": "/api/public/uploads/$1"
      },
      {
        "src": "/api/public/uploads/(.*).jpg",
        "dest": "/api/public/uploads/$1"
      },
      {
        "src": "/",
        "dest": "/api/public/home.php"
      },
      {
        "src": "/add",
        "dest": "/api/public/pages/add.php"
      },
      {
        "src": "/edit",
        "dest": "/api/public/pages/edit.php"
      },
      {
        "src": "/view",
        "dest": "/api/public/pages/view.php"
      },
      {
        "src": "/404",
        "dest": "/api/public/pages/404.php"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
  ],
