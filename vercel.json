{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { 
      "src": "/(.*)",
      "dest": "/api/index.php"
    },
    { 
      "src": "style.css",
      "dest": "/public/style.css"
    },
    { 
      "src": "uploads/(.*)png",
      "dest": "/public/uploads/$1"
    },
    { 
      "src": "uploads/(.*)jpg",
      "dest": "/public/uploads/$1"
    },
    { 
      "src": "/api/index.php",
      "dest": "/public/index.php"
    },
    { 
      "src": "portfolio/add.php",
      "dest": "/public/portfolio/add.php"
    },
    { 
      "src": "portfolio/delete.php",
      "dest": "/public/portfolio/delete.php"
    },
    { 
      "src": "portfolio/edit.php",
      "dest": "/public/portfolio/edit.php"
    },
    { 
      "src": "includes/db_connection.php",
      "dest": "/public/includes/db_connection.php"
    }
  ]
}
