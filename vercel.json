{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
      {
        "src": "/style.css",
        "dest": "/public/style.css"
      },
      {
        "src": "/uploads/(.*).png",
        "dest": "/public/uploads/$1"
      },
      {
        "src": "/uploads/(.*).jpg",
        "dest": "/public/uploads/$1"
      },
      {
        "src": "/pages/home.php",
        "dest": "/public/home.php"
      },
      {
        "src": "/pages/add.php",
        "dest": "/public/pages/add.php"
      },
      {
        "src": "/pages/edit.php",
        "dest": "/public/pages/edit.php"
      },
      {
        "src": "/pages/view.php",
        "dest": "/public/pages/view.php"
      },
      {
        "src": "/pages/404.php",
        "dest": "/public/pages/404.php"
      },      {
        "src": "/procces/add.php",
        "dest": "/public/procces/add.php"
      },  
      {
        "src": "/procces/edit.php",
        "dest": "/public/procces/edit.php"
      },
      {
        "src": "/procces/delete.php",
        "dest": "/public/procces/delete.php"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
  ]
}